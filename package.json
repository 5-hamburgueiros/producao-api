  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:ci": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:ci": "jest --ci --coverage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ./src/infra/configs/typeorm-migration.config.ts",
    "migrate": "ts-node -r tsconfig-paths/register src/common/utils/migration.ts",
    "typeorm:sync": "npm run typeorm -- schema:sync",
    "typeorm:log": "npm run typeorm -- schema:log",
    "typeorm:drop": "npm run typeorm schema:drop",
    "typeorm:reset": "npm run typeorm:drop && npm run typeorm:sync",
    "typeorm:migrate": "npm run typeorm -- migration:generate -p ./sql/migrations/$npm_config_name/$npm_config_name",
    "typeorm:create": "npm run typeorm -- migration:create",
    "typeorm:run": "npm run typeorm -- migration:run"
  },
  "dependencies": {
    "@bull-board/express": "^5.20.5",
